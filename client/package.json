{
  "name": "rectangle-packing-client",
  "version": "0.1.0",
  "description": "Ứng dụng netting liệu",
  "author": "Nguyễn Hoàng Long",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.3.4",
    "framer-motion": "^10.0.1",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-draggable": "^4.4.5",
    "react-resizable": "^3.0.5",
    "react-router-dom": "^6.8.1",
    "react-scripts": "5.0.1",
    "socket.io-client": "^4.7.2",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "cross-env BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "electron-dev": "concurrently \"npm start\" \"wait-on http://localhost:3000 && electron public/electron.cjs\"",
    "electron-pack": "npm run build && node -e \"require('fs').copyFileSync('public/electron.cjs', 'build/electron.cjs')\" && electron-builder"
  },
  "main": "build/electron.cjs",
  "homepage": "./",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:5000",
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "electron": "^25.0.0",
    "electron-builder": "^24.6.4",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.3.0",
    "wait-on": "^7.0.1"
  },
  "build": {
    "appId": "com.rectanglepacking.optimizer",
    "productName": "Tự động sắp xếp liệu",
    "directories": {
      "output": "dist"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    
    "icon": "build/icon.png", 
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico"
    }
  },
  "type": "module"
}
